<table class='table table-striped'>
  <% @songs.each do |song| %>
    <tr>
      <td>
        <a href="/songs/<%= song.id %>">
          <%= song.title %>
        </a>
      </td>
      <td><%= song.artist %></td>
      <td><%= song.album %></td>
      <td><%= song.genre %></td>
      <td><%= song.length %></td>
    </tr>
  <% end %>
</table>
